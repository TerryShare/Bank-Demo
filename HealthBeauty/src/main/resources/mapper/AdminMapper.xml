<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bank.dao.AdminDao">
		<!-- 登录 -->
	<select id="login" parameterType="org.bank.pojo.Admin"
		resultType="org.bank.pojo.Admin">
		select * from admin where name=#{name} and
		password=#{password}
	</select>
	
	
</mapper>